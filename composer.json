{
    "name": "codenix-sv/messari-api",
    "type": "library",
    "description": "PHP REST API client for messari.io",
    "homepage": "https://github.com/codenix-sv/messari-api",
    "keywords": ["client", "api", "php", "messari.io", "rest"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/codenix-sv/messari-api/issues?state=open",
        "source": "https://github.com/codenix-sv/messari-api"
    },
    "authors": [
        {
            "name": "Volodymyr Svyryd",
            "email": "codenix.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "codenix-sv/api-client": "^1.0",
        "nyholm/psr7": "^1.2",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "php-http/mock-client": "^1.3"
    },
    "autoload": {
        "psr-4": { "Codenixsv\\MessariApi\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Codenixsv\\MessariApi\\Tests\\": "tests/"}
    },
    "prefer-stable": true
}
